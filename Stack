class Stack {
  constructor() {
    this.items = []
    this.count = 0
  }
  push(element) {
    this.items[this.count] = element
    this.count += 1
  }
  pop() {
    if(!this.count) return undefined 
    let deletedItem = this.items[this.count-1]
    this.count = this.count - 1
    return deletedItem
  }
  peek() {
    return this.items[this.count-1]
  }
  isEmpty() {
    return this.count === 0
  }
  size() {
    return this.count
  }
  print() {
    return this.items
  }
  clear() {
    this.count = 0
    this.items = []
  }
}
