class Node {
    constructor(value) {
        this.value = value
        this.next = null
    })
}

class LinkedList {
    constructor() {
        this.size = 0
        this.root = null
    }
    add(value) {
        if (this.size === 0) {
            this.root = new Node(value)
            this.size += 1
            return true
        }
        let node = this.root
        let newNode = new Node(value)
        while (node.next) node = node.next
        node.next = newNode
        this.size += 1
        return true
    }
    getSize() {
        return this.size
    }
    print() {
        let result = []
        let node = this.root
        while (node) {
            result.push(node.value)
            node = node.next
        }
        console.log(result)
    }
}
